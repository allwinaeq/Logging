<?xml version="1.0" encoding="ISO-8859-1"?>

<Configuration>
    <Appenders>
        <Console name="pon">
            <PatternLayout pattern="%d [%t] %-5level %logger{36} - %msg%n"/>
        </Console>
    </Appenders>

    <Loggers>
        <Root level="error">
            <AppenderRef ref="pon"/>

        </Root>
    </Loggers>

</Configuration>

